<orders> <!-- Creating an order with the same ref as a system order will override the latest. -->
	<!-- Actuators -->
	<order ref="actuator_ax12_set_pos" duration="1.5">
		<message dest="/drivers/ax12" timeout="8">
			<param name="motor_id" type="int"/>
			<param name="position" type="int"/> <!-- Set either a preset or param-->
			<param name="speed" type="int" optional="true">0</param>
			<param name="mode" type="int" preset="true">0</param>
		</message>
		<response>
			<param name="success" type="bool" condition="==">1</param>
		</response>
	</order>

	<order ref="tower_load_atom" duration="2">
		<message dest="/actuators/ard_tower" timeout="8">
			<param name="load_content" type="int" preset="true">1</param>
		</message>
	</order>

	<order ref="tower_load_tower" duration="2">
		<message dest="/actuators/ard_tower" timeout="8">
			<param name="load_content" type="int" preset="true">2</param>
		</message>
	</order>

	<order ref="tower_unload_slider" duration="2">
		<message dest="/actuators/ard_tower" timeout="8">
			<param name="unload_content" type="int" preset="true">1</param>
		</message>
	</order>

	<order ref="tower_unload_pliers" duration="2">
		<message dest="/actuators/ard_tower" timeout="8">
			<param name="unload_content" type="int" preset="true">2</param>
		</message>
	</order>

	<order ref="sucker_on">
		<message dest="/actuators/ard_door" timeout="8">
			<param name="sucker_status" type="int" preset="true">1</param>
		</message>
	</order>

	<order ref="sucker_off">
		<message dest="/actuators/ard_door" timeout="8">
			<param name="sucker_status" type="int" preset="true">0</param>
		</message>
	</order>
</orders>
