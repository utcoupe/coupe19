<actions>
	<!-- Game Management actions -->
	<action ref="jack">
		<params/>
		<actions exec="all" order="linear">
			<tasklist repeat="3">
				<orderref ref="ask_for_jack"/>
			</tasklist>
			<orderref ref="wait_for_jack"/>
		</actions>
	</action>

	<action ref="game_start" name="Game start routine actions">
		<params></params>
		<actions>
			<orderref ref="set_robot_pos">
				<position_waypoint>way_spawn_pr</position_waypoint>
			</orderref>

			<actionref ref="jack"/>

			<orderref ref="timer_start">
				<duration>97</duration>
			</orderref>

			<orderref ref="set_game_status_ingame"/>
		</actions>
	</action>

	<!-- Game actions -->
	<action ref="load_side_atoms">
		<params></params>
		<actions exec="all" order="linear">
			<orderref ref="navigator_goto_waypoint" name="first turn">
				<waypoint_name>way_first_turn</waypoint_name>
				<disable_pathfinder>1</disable_pathfinder>
			</orderref>

			<orderref ref="navigator_goto_waypoint" name="goto atom1">
				<waypoint_name>way_atom1</waypoint_name>
				<disable_pathfinder>1</disable_pathfinder>
			</orderref>
			<!-- <orderref ref="tower_load_atom" needsprevious="true"/> -->
			<orderref ref="map_transfer" name="remove atom from map">
				<mode>0</mode> <!-- select object only -->
				<old_path>map/spawn_atoms/spawn_atoms_rd/atom1</old_path>
				<new_path>robot/tower</new_path>
			</orderref>

			<orderref ref="navigator_goto_waypoint" name="goto atom2">
				<waypoint_name>way_atom2</waypoint_name>
				<disable_pathfinder>1</disable_pathfinder>
			</orderref>
			<!-- <orderref ref="tower_load_atom" needsprevious="true"/> -->
			<orderref ref="map_transfer" name="remove atom from map">
				<mode>0</mode> <!-- select object only -->
				<old_path>map/spawn_atoms/spawn_atoms_gd/atom2</old_path>
				<new_path>robot/tower</new_path>
			</orderref>

			<orderref ref="navigator_goto_waypoint" name="goto atom3">
				<waypoint_name>way_atom3</waypoint_name>
				<disable_pathfinder>1</disable_pathfinder>
			</orderref>
			<!-- <orderref ref="tower_load_atom" needsprevious="true"/> -->
			<orderref ref="map_transfer" name="remove atom from map">
				<mode>0</mode> <!-- select object only -->
				<old_path>map/spawn_atoms/spawn_atoms_bd/atom3</old_path>
				<new_path>robot/tower</new_path>
			</orderref>
		</actions>
	</action>

	<action ref="load_chaos_atoms">
		<params></params>
		<actions exec="all" order="linear">
			<orderref ref="delay"/>
		</actions>
	</action>

	<action ref="load_gr_tower">
		<params></params>
		<actions exec="all" order="linear">
			<orderref ref="navigator_goto_waypoint" name="approach acc">
				<waypoint_name>way_tower</waypoint_name>
			</orderref>
			<orderref ref="delay"> <!-- TODO remove -->
				<duration>2</duration>
			</orderref>
		</actions>
	</action>

	<action ref="unload_tower">
		<params></params>
		<actions exec="all" order="linear">
			<orderref ref="navigator_goto_waypoint" name="approach acc">
				<waypoint_name>way_approach_acc</waypoint_name>
			</orderref>
			<orderref ref="asserv_pwm" name="smat pwm on acc (reversed)">
				<left>-150</left>
				<right>-150</right>
				<duration>3</duration>
				<auto_stop>1</auto_stop>
			</orderref>

			<!-- <orderref ref="tower_unload_tower"/> -->
			<orderref ref="delay"> <!-- TODO remove -->
				<duration>2</duration>
			</orderref>

			<orderref ref="navigator_goto_waypoint" name="approach acc">
				<waypoint_name>way_approach_acc</waypoint_name>
				<direction>1</direction>
			</orderref>
		</actions>
	</action>

	<action ref="load_goldenium">
		<params></params>
		<actions exec="all" order="linear">
			<orderref ref="navigator_goto_waypoint" name="approach goldenium at 45deg">
				<waypoint_name>way_goldenium_approach</waypoint_name>
				<disable_pathfinder>0</disable_pathfinder>
			</orderref>
			<orderref ref="navigator_goto_waypoint" name="touch goldenium et 45deg">
				<waypoint_name>way_goldenium</waypoint_name>
				<disable_pathfinder>1</disable_pathfinder>
				<direction>0</direction> <!-- backwards -->
			</orderref>
			<orderref ref="navigator_goto_waypoint" name="approach goldenium at 45deg">
				<waypoint_name>way_goldenium_approach</waypoint_name>
				<disable_pathfinder>0</disable_pathfinder>
			</orderref>
		</actions>
	</action>

	<action ref="unload_goldenium">
		<params></params>
		<actions exec="all" order="linear">
			<orderref ref="navigator_goto_waypoint" name="approach goldenium at 45deg">
				<waypoint_name>way_unload_goldenium_approach</waypoint_name>
				<disable_pathfinder>0</disable_pathfinder>
			</orderref>
			<orderref ref="navigator_goto_waypoint" name="touch goldenium et 45deg">
				<waypoint_name>way_unload_goldenium</waypoint_name>
				<disable_pathfinder>1</disable_pathfinder>
				<direction>0</direction> <!-- backwards -->
			</orderref>
			<orderref ref="navigator_goto_waypoint" name="approach goldenium at 45deg">
				<waypoint_name>way_unload_goldenium_approach</waypoint_name>
				<disable_pathfinder>0</disable_pathfinder>
			</orderref>
		</actions>
	</action>

	<!-- Helpers actions -->
	<action ref="goto_spawn" name="Goto spawn">
		<params></params>
		<actions exec="all" order="linear">
			<!--orderref ref="navigator_goto_waypoint" name="approach spawn">
				<waypoint_name>way_spawn_approach</waypoint_name>
			</orderref-->

			<orderref ref="navigator_goto_waypoint" name="goto spawn" timeout="40">
				<waypoint_name>way_spawn_pr</waypoint_name>
			</orderref>
		</actions>
	</action>
</actions>
